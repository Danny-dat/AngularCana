{
  "name": "cannatrack-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "node ./node_modules/@angular/cli/bin/ng.js",
    "start": "node ./node_modules/@angular/cli/bin/ng.js serve",
    "build": "node ./node_modules/@angular/cli/bin/ng.js build --configuration production --base-href /",
    "watch": "node ./node_modules/@angular/cli/bin/ng.js build --watch --configuration development",
    "test": "node ./node_modules/@angular/cli/bin/ng.js test",
    "test:ci": "node ./node_modules/@angular/cli/bin/ng.js test --no-watch --no-progress --browsers=ChromeHeadless --coverage",
    "serve:ssr:cannatrack-angular": "node dist/cannatrack-angular/server/server.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.5",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/fire": "^20.0.1",
    "@angular/forms": "^20.3.0",
    "@angular/material": "^20.2.5",
    "@angular/platform-browser": "^20.3.0",
    "@angular/platform-server": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@angular/ssr": "^20.3.3",
    "chart.js": "^4.5.0",
    "express": "^5.1.0",
    "leaflet": "^1.9.4",
    "qrcode": "^1.5.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.3.3",
    "@angular/cli": "^20.3.3",
    "@angular/compiler-cli": "^20.3.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^20.17.19",
    "@types/qrcode": "^1.5.5",
    "baseline-browser-mapping": "^2.9.14",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}
